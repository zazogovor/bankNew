
<%= bootstrap_form_tag url: '/employeelogin' do |f| %>
	<div class = "row">
		<div class="col-md-5">
			<h1>Employee login</h1>
			<%= f.email_field :email, placeholder: 'name@example.com' %>
			<%= f.password_field :password %>
			<input class = "btn btn-default" type = "submit" vlue = "Log in" %>
		</div>
	</div>
<% end %>

